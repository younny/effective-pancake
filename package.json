{
  "name": "EffectivePancake",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run lint && npm run flow && npm run test:unit",
    "test:unit": "jest",
    "flow": "flow",
    "flow-stop": "flow stop",
    "lint": "eslint app"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "immutable": "^4.0.0-rc.9",
    "ramda": "^0.25.0",
    "react": "^16.3.0-alpha.1",
    "react-native": "^0.54.2",
    "react-native-navigation": "^1.1.411",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.65.0",
    "jest": "22.4.2",
    "react-test-renderer": "^16.3.0-alpha.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__test__/setup.js"
    ],
    "setupFiles": [
      "./__test__/setup"
    ]
  }
}
